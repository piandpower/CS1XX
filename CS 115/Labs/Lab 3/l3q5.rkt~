#reader(lib "htdp-beginner-reader.ss" "lang")((modname l3q5) (read-case-sensitive #t) (teachpacks ((lib "potatohead.rkt" "installed-teachpacks"))) (htdp-settings #(#t constructor repeating-decimal #f #t none #f ((lib "potatohead.rkt" "installed-teachpacks")))))
(define wk-rate 74.95)
(define dy-rate 14.95)

(define (airport-parking day-num)
  (cond [ (< day-num 7) (* day-num wk-rate)]
        [ (> day-num 7) (+ (* (quotient day-num 7) wk-rate)
                           (* (remainder day-num 7) dy-rate)
                        
  ...)