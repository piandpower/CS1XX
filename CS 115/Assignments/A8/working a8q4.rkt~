(define (my-aexp-fun ex)
  (cond 
    [(number? ex)...]
    [(ae? ex) ...(ae-fun ex)...
              ...(my-aexplist-fun (ae-args ex))...]))

(define (my-aexplist-fun exlist)
  (cond
    [(empty? exlist)...]
    [(cons? exlist)...(my-aexp-fun (first exlist))...
                   ...(my-explist-fun (rest exlist))...]))

(define (my-eval ex)
  (cond
    [(number? ex) ex]
    [else
     (apply (ae-fn ex) (ae-args ex))]))

